{
  "name": "johnpaulmedina/notes-field",
  "description": "This Laravel Nova package adds a notes field to Nova fields.",
  "keywords": [
    "laravel",
    "nova",
    "outl1ne",
    "field"
  ],
  "authors": [
    {
      "name": "John Paul Medina",
      "email": "jp@livingdonorproject.org",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "outl1ne/nova-translations-loader": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Johnpaulmedina\\NotesField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Johnpaulmedina\\NotesField\\NotesFieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
